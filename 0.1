    for _, obj in ipairs(game.Workspace.Ignored.Shop:GetDescendants()) do
        if obj:IsA('BasePart') then
            local distance = (root.Position - obj.Position).Magnitude
            if distance < studs then
                studs = distance
                part = obj
            end
        end
    end
    return part and part.Parent
end

